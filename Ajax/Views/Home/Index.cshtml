

@model IEnumerable<Ajax.Controllers.Book>
<div id="search_results">
    @if (@Model != null)
    {
        <h3>Все книги автора @Model.First().Author</h3>
        <ul>
            @foreach (var item in Model)
            {
                <li>@item.Title</li>
            }
        </ul>
    }
</div>

